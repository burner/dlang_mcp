{
	"targetName": "dlang_mcp",
	"authors": [
		"burner"
	],
	"lflags-linux": [
		"-rpath",
		"$$ORIGIN/../lib"
	],
	"license": "BSL-1.0",
	"targetPath": "bin",
	"copyright": "Copyright Â© 2026, burner",
	"name": "dlang_mcp",
	"description": "MCP server for D language tools with semantic package search",
	"dependencies": {
		"d2sqlite3": "~>1.0.0",
		"vibe-d:http": "~>0.10.3",
		"bindbc-onnxruntime": "~>1.2.0",
		"libdparse": "~>0.25.1"
	},
	"targetType": "executable",
	"configurations": [
		{
			"mainSourceFile": "source/app.d",
			"targetType": "executable",
			"targetName": "dlang_mcp",
			"versions": [
				"VibeDisableCommandLineParsing"
			],
			"name": "default"
		},
		{
			"targetName": "dlang_mcp_test",
			"mainSourceFile": "tests/runner.d",
			"versions": [
				"TestMode"
			],
			"name": "test",
			"sourcePaths": [
				"source",
				"tests"
			],
			"importPaths": [
				"source",
				"tests"
			],
			"targetType": "executable"
		}
	]
}
